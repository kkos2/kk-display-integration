{
  "name": "kk-display-integration",
  "version": "1.0.0",
  "description": "Integration engine for KK connecting OS2 Display with API sources.",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "(test -f .env || cp dist.env .env) && nest start",
    "start:dev": "(test -f .env || cp dist.env .env) && nest start --watch",
    "start:debug": "(test -f .env || cp dist.env .env) && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint:check": "eslint '{src,apps,libs,test}/**/*.ts'",
    "lint:fix": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "echo 'No e2e tests implemented yet.'"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "8.2.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "8.2.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/schedule": "^2.0.1",
    "@nestjs/swagger": "^5.2.1",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "swagger-ui-express": "^4.5.0",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "8.1.5",
    "@nestjs/schematics": "8.0.8",
    "@types/cron": "^2.0.0",
    "@types/jest": "^27.5.2",
    "@types/node": "16.11.26",
    "@types/supertest": "2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.8",
    "ts-node": "10.4.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "^4.3.5"
  }
}
